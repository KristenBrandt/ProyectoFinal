<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".signup">

    <Button
        android:id="@+id/create_button"
        android:layout_width="318dp"
        android:layout_height="73dp"
        android:layout_marginBottom="264dp"
        android:layout_marginEnd="37dp"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="37dp"
        android:layout_marginStart="33dp"
        android:text="@string/SignUp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Sign UP" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="310dp"
        android:layout_height="47dp"
        android:layout_marginBottom="6dp"
        android:layout_marginEnd="37dp"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="37dp"
        android:layout_marginStart="38dp"
        android:ems="10"
        android:hint="@string/error_field_required"
        android:inputType="textEmailAddress"
        android:text="@string/Username"
        app:layout_constraintBottom_toTopOf="@+id/editText2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.66"
        tools:text="Username" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="310dp"
        android:layout_height="45dp"
        android:layout_marginBottom="28dp"
        android:layout_marginEnd="37dp"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="37dp"
        android:layout_marginStart="37dp"
        android:ems="10"
        android:hint="@string/error_field_required"
        android:inputType="textPassword"
        android:text="@Password"
        app:layout_constraintBottom_toTopOf="@+id/create_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="Password" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="25dp"
        android:layout_height="43dp"
        android:layout_marginBottom="20dp"
        android:layout_marginEnd="187dp"
        android:layout_marginLeft="187dp"
        android:layout_marginRight="187dp"
        android:layout_marginStart="187dp"
        android:layout_marginTop="48dp"
        android:text="@string/o"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/create_button"
        tools:text="O" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="37dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="37dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="24dp"
        android:text="@string/Ya"
        android:textSize="22sp"
        app:layout_constraintBottom_toTopOf="@+id/account_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:text="Ya tengo una cuenta en Dogtor" />

    <Button
        android:id="@+id/account_button"
        android:layout_width="324dp"
        android:layout_height="62dp"
        android:layout_marginBottom="72dp"
        android:layout_marginEnd="37dp"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="37dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="13dp"
        android:text="@string/logn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:text="Log In" />
</android.support.constraint.ConstraintLayout>